        "bcryptjs": "^3.0.2",
        "body-parser-xml": "^2.0.5",
        "callsites": "^4.1.0",
        "cli-table3": "catalog:",
        "cookie-parser": "^1.4.7",
        "date-fns": "^4.1.0",
        "filesize": "^10.1.6",
        "fs-extra": "^11.3.1",
        "js-yaml": "^4.1.0",
        "mammoth": "^1.10.0",
        "node-machine-id": "^1.1.12",
        "node-segment": "^0.0.2",
        "pdf-parse": "^1.1.4",
        "pg": "catalog:api",
        "pg-connection-string": "^2.9.1",
        "radash": "^12.1.1",
        "reflect-metadata": "^0.2.2",
