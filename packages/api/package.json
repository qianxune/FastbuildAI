        "@eslint/js": "^9.18.0",
        "@nestjs/cli": "^11.0.10",
        "@nestjs/schematics": "^11.0.7",
        "@nestjs/testing": "^11.1.6",
        "@swc/cli": "^0.7.8",
        "@types/adm-zip": "^0.5.7",
        "@types/body-parser-xml": "^2.0.5",
        "@types/cookie-parser": "^1.4.9",
        "@types/jest": "^30.0.0",
        "@types/ms": "^2.1.0",
        "@types/pg": "^8.15.5",
        "@types/supertest": "^6.0.2",
        "eslint-config-prettier": "^10.0.1",
        "eslint-plugin-prettier": "^5.2.2",
        "globals": "^16.0.0",
        "jest": "^30.0.0",
        "nodemon": "^3.1.9",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-loader": "^9.5.2",
        "tsconfig-paths": "^4.2.0"
    },
    "overrides": {
        "ms": "2.1.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "engines": {
        "node": ">=22.20.x <23"
    }
}